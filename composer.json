{
    "name": "beckup/connect-rest-api-bundle",
    "description": "REST API connection bundle for Symfony with authentication support",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["symfony", "bundle", "rest", "api", "http", "client", "authentication"],
    "homepage": "https://github.com/beckup/connect-rest-api-bundle",
    "authors": [
        {
            "name": "BeckUp Team",
            "email": "info@beckup.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/framework-bundle": "^6.0|^7.0",
        "symfony/dependency-injection": "^6.0|^7.0",
        "symfony/http-client": "^6.0|^7.0"
    },

    "autoload": {
        "psr-4": {
            "BeckUp\\ConnectRestApiBundle\\": "src/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
